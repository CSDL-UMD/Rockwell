[Unit]
Description=twauth-web.py service file
After=network.target

[Service]
User=ubuntu
WorkingDirectory=/home/ubuntu/Rockwell/backend/src/authorizer
ExecStart=/home/ubuntu/Rockwell/backend/.venv/bin/gunicorn -b 127.0.0.1:5000 -t 600 "twauth-web-qualtrics:app" 
Restart=always

[Install]
WantedBy=multi-user.target     
